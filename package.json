{
  "name": "axios-request-wrapper",
  "version": "0.1.0",
  "description": "Pluggable axios wrapper with crypto/signature adapters, retries and logging.",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && node scripts/build-cjs.js",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "axios": ">=1.2.0"
  },
  "devDependencies": {
    "axios": "^1.7.7",
    "typescript": "^5.4.0",
    "rimraf": "^5.0.5"
  },
  "keywords": [
    "axios",
    "http",
    "crypto",
    "signature",
    "retry",
    "logging"
  ]
}
